https://steamcommunity.com:14304 https://www.steamcommunity.com:14304 {
    tls steamcommunity.crt steamcommunity.key
    @steamcommunityrp {
    path /comment/*
    path /forum/*
    path *discussions*
    path *tradingforum*
    path *eventcomments*
    path /chat/beginfileupload/*
    }
    reverse_proxy @steamcommunityrp https://str001.steam302.xyz https://str002.steam302.xyz https://str003.steam302.xyz {
    header_up Host steamcommunity.com
			transport http {
			tls
			tls_server_name statuspage.akamaized.net
			#tls_insecure_skip_verify
		}
    }
    reverse_proxy https://steamuserimages-a.akamaihd.net https://steamstore-a.akamaihd.net https://steamcommunity-a.akamaihd.net https://eaassets-a.akamaihd.net https://steamcommunity-a.akamaihd.net {
    dynamic a steamuserimages-a.akamaihd.net 443
    dynamic a steamstore-a.akamaihd.net 443
    dynamic a steamcommunity-a.akamaihd.net 443
    dynamic a eaassets-a.akamaihd.net 443
    dynamic a steamcommunity-a.akamaihd.net 443
    header_up Host steamcommunity.com
    lb_policy random_choose 5
		transport http {
			tls
			tls_server_name img-s-msn-com.akamaized.net
			#tls_insecure_skip_verify
		}
    }
}

